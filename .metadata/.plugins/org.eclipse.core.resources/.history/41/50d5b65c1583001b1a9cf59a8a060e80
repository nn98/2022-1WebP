package main;

import java.io.File;
import java.io.FileReader;


public class Counting {

	public static void main(String[] args) {
		try {
			File f=new File("C:/Non/main.txt");
			FileReader fr=new FileReader(f);
			int c=0;
			while((c=fr.read())!=-1)System.out.println((char)c);
			fr.close();
		}catch(Exception e) {
			e.getStackTrace();
		}
	}

}
