public class Counting {

	public static void main(String[] args) {
		try {
			OutputStream o=new FileOutputStream("C:/Non/main.txt");
			String s="∏È\t22\n∏È√ﬂ\t2\nπ‰\t30\nπ‰√ﬂ\t6";
			byte[]b=s.getBytes();
			o.write(b);
		}catch(Exception e) {
			e.getStackTrace();
		}
	}

}
